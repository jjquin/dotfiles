#
# ZSH Global Aliases
#

	# Command line head / tail shortcuts
	alias	-g H='| head' \
		-g T='| tail' \
		-g G='| grep' \
		-g L="| less" \
		-g M="| most" \
		-g LL="2>&1 | less" \
		-g CA="2>&1 | cat -A" \
		-g NE="2> /dev/null" \
		-g NUL="> /dev/null 2>&1" \
		-g P="2>&1| pygmentize -l pytb"


